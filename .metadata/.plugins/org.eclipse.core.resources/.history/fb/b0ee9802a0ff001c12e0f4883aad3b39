
public class MediaSalarial {
	public static void main(String[] args) {
		//varaiveis
		double salario1,salario2,salario3,salario4, media;
		//Exemplo de salarios:
		salario1 = 1000;
		
		salario2 = 2000;
		
		salario3 = 4000;
		
		salario4 = 8000;
		
		//calculo da media:
		
		media = (salario1 + salario2 + salario3 + salario4) / 4;
		
		System.out.println("A media de salarios da empresa Ã© de: " + media );
		
	}

}
